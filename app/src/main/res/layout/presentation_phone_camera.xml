<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">



    <ImageView
        android:id="@+id/iv_phone_camera"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <SurfaceView
        android:id="@+id/surface_presentation"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <TextView
        android:id="@+id/tv_top"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:gravity="center"
        android:text="视频监控"
        android:textSize="@dimen/x48"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent="0.138888888"
        style="@style/presentNameItem"
        />

    <!--返回按钮-->
    <ImageButton
        android:id="@+id/ib_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintWidth_percent="0.07"
        app:layout_constraintHeight_percent="0.138888888"
        style="@style/nullbg"
        android:src="@drawable/back"/>

    <TextView
        android:id="@+id/tv_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="返回"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="@dimen/x48"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/ib_back"
        app:layout_constraintWidth_percent="0.06"
        app:layout_constraintHeight_percent="0.138888888"
        style="@style/nullbg"
        android:src="@drawable/back"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHeight_percent="0.379269"
        app:layout_constraintDimensionRatio="3.5:5.2"
        android:background="@color/presentationbg"/>

    <ImageView
        android:id="@+id/iv_success_curr"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHeight_percent="0.379269"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/tv_presentation_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/font_white"
        android:textStyle="bold"
        android:textSize="@dimen/x42"
        android:gravity="center|left"
        android:paddingStart="@dimen/x15"
        android:paddingTop="@dimen/x42"
        app:layout_constraintTop_toTopOf="@+id/iv_success_curr"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_curr"
        app:layout_constraintWidth_percent="0.15"
        app:layout_constraintHeight_percent="0.13"
        android:background="@color/presentationbg"
        />

    <TextView
        android:id="@+id/tv_presentation_company"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/font_white"
        android:textStyle="bold"
        android:textSize="@dimen/x28"
        android:gravity="center|left"
        android:paddingStart="@dimen/x15"
        app:layout_constraintTop_toBottomOf="@+id/tv_presentation_name"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_curr"
        app:layout_constraintWidth_percent="0.15"
        app:layout_constraintHeight_percent="0.083"
        android:background="@color/presentationbg"
        />

    <TextView
        android:id="@+id/tv_presentation_deputation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/font_white"
        android:textStyle="bold"
        android:textSize="@dimen/x28"
        android:gravity="center|left"
        android:paddingStart="@dimen/x15"
        app:layout_constraintTop_toBottomOf="@+id/tv_presentation_company"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_curr"
        app:layout_constraintWidth_percent="0.15"
        app:layout_constraintHeight_percent="0.083"
        android:background="@color/presentationbg"
        />

    <TextView
        android:id="@+id/tv_presentation_auth"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/font_green"
        android:textStyle="bold"
        android:textSize="@dimen/x28"
        android:gravity="center|left"
        android:paddingStart="@dimen/x15"
        app:layout_constraintTop_toBottomOf="@+id/tv_presentation_deputation"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_curr"
        app:layout_constraintWidth_percent="0.15"
        android:background="@color/presentationbg"
        app:layout_constraintBottom_toBottomOf="@+id/iv_success_curr"
        />

    <ImageView
        android:id="@+id/iv_success_last1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name1"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name1"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name1"
        app:layout_constraintDimensionRatio="3.5:5.2"
        />

    <TextView
        android:id="@+id/iv_success_last_name1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_presentation_name"
        style="@style/presentNameItem1"
        />

    <ImageView
        android:id="@+id/iv_success_last2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name2"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name2"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name2"
        app:layout_constraintDimensionRatio="3.5:5.2"
        />

    <TextView
        android:id="@+id/iv_success_last_name2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name1"
        style="@style/presentNameItem1"
        />

    <ImageView
        android:id="@+id/iv_success_last3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name3"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name3"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name3"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name2"
        style="@style/presentNameItem1"/>

    <ImageView
        android:id="@+id/iv_success_last4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name4"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name4"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name4"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name3"
        style="@style/presentNameItem1"/>
    
    <ImageView
        android:id="@+id/iv_success_last5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name5"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name5"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name5"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name4"
        style="@style/presentNameItem1"/>

    <ImageView
        android:id="@+id/iv_success_last6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name6"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name6"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name6"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name5"
        style="@style/presentNameItem1"/>

    <ImageView
        android:id="@+id/iv_success_last7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name7"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name7"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name7"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name6"
        style="@style/presentNameItem1"/>

    <ImageView
        android:id="@+id/iv_success_last8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_success_last_name8"
        app:layout_constraintLeft_toLeftOf="@+id/iv_success_last_name8"
        app:layout_constraintRight_toRightOf="@+id/iv_success_last_name8"
        app:layout_constraintDimensionRatio="3.5:5.2"/>

    <TextView
        android:id="@+id/iv_success_last_name8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/iv_success_last_name7"
        style="@style/presentNameItem1"/>

    <ImageView
        android:id="@+id/iv_test_snap"
        android:layout_width="@dimen/x300"
        android:layout_height="@dimen/x200"
        android:visibility="invisible"
        app:layout_constraintRight_toRightOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>
